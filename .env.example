# Tinkerer Vote - Environment Configuration
# Copy this file to .env and fill in the values
# IMPORTANT: Never commit .env to version control

# Application
NODE_ENV=production
PORT=3000

# Session secret - generate with: openssl rand -hex 32
SESSION_SECRET=your-secret-here-generate-with-openssl-rand-hex-32

# Discord OAuth
DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret
DISCORD_REDIRECT_URI=https://tinkerer.vote/auth/callback

# Tinkerer Club Discord Server ID (Guild ID)
TINKERER_GUILD_ID=your-server-id

# Database
DB_HOST=host.docker.internal
DB_PORT=3306
DB_USER=tinkerer_vote_user
DB_PASSWORD=your-database-password
DB_NAME=tinkerer_vote
